{
  "name": "waffler/generator",
  "description": "Generator Component",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Erick de Menezes",
      "email": "erickmenezes.dev@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Waffler\\Component\\Generator\\": "src",
      "Waffler\\Component\\Generator\\Generated\\": "generated"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Waffler\\Component\\Generator\\Tests\\": "tests"
    }
  },
  "require": {
    "php": "^8.4",
    "waffler/attributes": "^4.0.0",
    "waffler/helpers": "^4.0.0",
    "waffler/http-client-contracts": "^4.0.0",
    "waffler/generator-contracts": "^4.0.0",
    "nette/php-generator": "^4.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.0",
    "mockery/mockery": "^1.4",
    "jetbrains/phpstorm-attributes": "dev-master"
  },
  "scripts": {
    "phpunit": "@php ./vendor/bin/phpunit",
    "test": "@phpunit"
  },
  "config": {
    "allow-plugins": {
      "phpunit/phpunit": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "4.1.x-dev",
      "dev-main": "4.1.x-dev"
    }
  }
}
